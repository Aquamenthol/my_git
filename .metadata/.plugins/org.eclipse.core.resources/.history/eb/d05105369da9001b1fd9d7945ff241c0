package testjava;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		sc.close();

		if(-10000 <= a && a <= 10000 && -10000 <= b && b <= 10000) {
			if (a < b) System.out.println("<");
			if (a > b) System.out.println(">");
			if (a == b) System.out.println("==");
			}
		}
		
		
		
		
	}
}