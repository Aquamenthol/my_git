package testjava;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int h = sc.nextInt();
		int m = sc.nextInt();
		sc.close();

		if (0 <= h && h <= 23 && 0 <= m && m <= 59) {
			if (h == 0) {
				h = 24 -1;
			System.out.print(h);
			System.out.print(m + 15);
			}
			if (h == 23) {
				h = h - 1;
				System.out.println(h);
				System.out.print(m + 15);
			}
		}

	}
}