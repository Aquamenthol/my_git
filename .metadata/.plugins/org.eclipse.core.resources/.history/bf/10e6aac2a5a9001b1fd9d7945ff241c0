package testjava;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int gugu = sc.nextInt();
		for (int i = 1; i <= 9; i++) {
			gugu = gugu * i;
			System.out.println(gugu + "*" + i + "=" + gugu);
			gugu = sc.nextInt();
		}
	}
}