import javax.swing.*;

public class SwingWindowFrameMain2 extends WindowAdapter implements ActionListener{
	private JFame f;
	private JFame b;
	
	public class SwingWindowFrame2
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		f = new JFrame("windowadapter 테스트");
		b = new JButton("제목 바꾸기"); 
		Container c = f.getContentPane();
	}

}
